{
    "name": "swooletw/hyperf-cache",
    "type": "library",
    "description": "The cache package for Hyperf.",
    "license": "MIT",
    "keywords": [
        "php",
        "hyperf",
        "cache",
        "swoole",
        "laravel-hyperf"
    ],
    "authors": [
        {
            "name": "Albert Chen",
            "email": "albert@laravel-hyperf.com",
            "homepage": "https://albert-chen.com"
        }
    ],
    "support": {
        "issues": "https://github.com/swooletw/hyperf-packages/issues",
        "source": "https://github.com/swooletw/hyperf-packages"
    },
    "autoload": {
        "psr-4": {
            "SwooleTW\\Hyperf\\Cache\\": "src/"
        },
        "files": [
            "src/Functions.php"
        ]
    },
    "require": {
        "php": "^8.1",
        "hyperf/config": "~3.1.0",
        "hyperf/support": "~3.1.0",
        "laravel/serializable-closure": "^1.3",
        "psr/simple-cache": "^3.0"
    },
    "suggest": {
        "hyperf/redis": "Required to use redis driver. (~3.1.0)",
        "swooletw/cache": "Required to use file lock. (~3.1.0)"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "hyperf": {
            "config": "SwooleTW\\Hyperf\\Cache\\ConfigProvider"
        },
        "branch-alias": {
            "dev-main": "3.1-dev"
        }
    }
}
